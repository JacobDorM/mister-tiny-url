{
  "compilerOptions": {
    "target": "es6",

    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    // "checkJs": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "declaration": false,
    "sourceMap": true
  },
  "include": ["./src"],
  "exclude": ["./node_modules", "./logs","./dist"]
}
